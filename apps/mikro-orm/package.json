    "@mikro-orm/reflection": "4.5.7",
    "@mikro-orm/sql-highlighter": "1.0.1",
    "@nestjs/common": "8.0.5",
    "@nestjs/core": "8.0.5",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.0.0",
    "@nestjs/platform-express": "8.0.5",
    "@nestjs/swagger": "5.0.8",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "pg": "8.6.0",
    "rxjs": "7.2.0",
    "swagger-ui-express": "4.1.6",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@mikro-orm/cli": "4.5.7",
    "@nestjs/testing": "8.0.5",
    "@trejgun/eslint-config": "0.1.5",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.41",
    "@types/passport": "1.0.6",
    "@types/passport-jwt": "3.0.5",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "eslint": "7.24.0",
    "eslint-config-prettier": "8.2.0",
    "eslint-config-standard": "16.0.2",
    "eslint-import-resolver-ts": "0.4.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-standard": "4.1.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "ts-jest": "26.5.5",
    "ts-node": "9.1.1",
    "tsc-watch": "4.2.9",
    "typescript": "4.2.4"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "name": "mikro-orm-for-nestjs",
  "scripts": {
    "build": "tsc",
    "entity:create": "npm run typeorm -- entity:create -n",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "database:create": "npx mikro-orm schema:create -r",
    "database:update": "npx mikro-orm schema:update -r",
    "database:drop": "npx mikro-orm schema:drop -r",
    "prebuild": "rm -rf dist/",
    "prod": "NODE_ENV=production node .",
    "start": "NODE_ENV=development tsc-watch --onSuccess \"node .\" --onFailure \"echo Compilation Failed\"",
    "test": "NODE_ENV=test jest --config jest.config.json --detectOpenHandles"
  },
  "title": "JWT based authorization for Nest.js",
  "version": "0.0.1"
}
