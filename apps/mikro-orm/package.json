    "@mikro-orm/reflection": "5.2.3",
    "@mikro-orm/sql-highlighter": "1.0.1",
    "@nestjs/common": "8.4.7",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "8.4.7",
    "@nestjs/jwt": "8.0.1",
    "@nestjs/passport": "8.2.2",
    "@nestjs/platform-express": "8.4.7",
    "@nestjs/swagger": "5.2.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "pg": "8.7.3",
    "rxjs": "7.5.5",
    "swagger-ui-express": "4.4.0",
    "ts-node": "10.8.2",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@gemunion/cross-env": "0.0.4",
    "@gemunion/eslint-config": "1.0.5",
    "@mikro-orm/cli": "5.2.3",
    "@nestjs/testing": "8.4.7",
    "@types/express": "4.17.13",
    "@types/jest": "27.5.2",
    "@types/node": "16.11.43",
    "@types/passport": "1.0.9",
    "@types/passport-jwt": "3.0.6",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "16.0.3",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.5.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.0",
    "jest": "27.5.1",
    "prettier": "2.7.1",
    "ts-jest": "27.1.5",
    "tsc-watch": "4.6.2",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=12.13"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "mikro-orm": {
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ],
    "useTsNode": true
  },
  "name": "@gemunion/mikro-orm",
  "repository": {
    "type": "git",
    "url": "https://github.com/gemunion/nestjs-database"
  },
  "scripts": {
    "build": "tsc",
    "database:create": "npx mikro-orm schema:create -r",
    "database:drop": "npx mikro-orm schema:drop -r",
    "database:update": "npx mikro-orm schema:update -r",
    "entity:create": "npm run typeorm -- entity:create -n",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --fix",
    "prebuild": "rm -rf dist/",
    "prod": "cross-env NODE_ENV=${NODE_ENV:=production} node .",
    "start": "cross-env NODE_ENV=${NODE_ENV:=development} tsc-watch --onSuccess \"node .\" --onFailure \"echo Compilation Failed\"",
    "test": "cross-env NODE_ENV=${NODE_ENV:=test} jest --config jest.config.json --detectOpenHandles"
  },
  "title": "MikroOrm for Nest.js",
  "version": "0.0.1"
}
