  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint . --fix",
    "prebuild": "rimraf dist/",
    "prod": "cross-env NODE_ENV=${NODE_ENV:=production} node .",
